{
  "name": "@harmonyjs/accessor-mongoose",
  "version": "1.0.0-rc.12",
  "publishConfig": {
    "access": "public"
  },
  "mixt": {
    "dist": "./pkg"
  },
  "@pika/pack": {
    "pipeline": [
      [
        "@pika/plugin-ts-standard-pkg"
      ],
      [
        "@pika/plugin-build-node"
      ],
      [
        "@pika/plugin-build-web"
      ]
    ]
  },
  "description": "Gatsby controller for Harmony",
  "main": "dist/index.js",
  "scripts": {
    "mixt:build": "pika build",
    "mixt:watch": "nodemon -e ts,js,tsx,jsx,json,yml --watch src --exec \"npm run build\"",
    "mixt:release": "cd ./pkg && npm publish"
  },
  "author": "Jérémie van der Sande <jeremie.vandersande@gmail.com>",
  "license": "ISC",
  "gitHead": "a896e4656e71ab6dc197b1b0f88ac1635582a459",
  "devDependencies": {
    "typescript": "^3.5.3"
  },
  "dependencies": {
    "mongoose": "^5.7.7"
  }
}
