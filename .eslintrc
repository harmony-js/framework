{
  "parser": "typescript-eslint-parser",
  "parserOptions": {
    "sourceType": "module",
    "allowImportExportEverywhere": true
  },
  "extends": [
    "@foundationjs/eslint"
  ],
  "rules": {
    "react/no-unescaped-entities": "off",
    "no-unused-vars": "off"
  },
  "plugins": [ "typescript" ]
}
